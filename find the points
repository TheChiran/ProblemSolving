#include <iostream>

using namespace std;

int main()
{
    int px,
        py,
        qx,
        qy,
        rx,
        ry,
        n;
    scanf("%d",&n);
    while(n>0){
        scanf("%d %d %d %d",&px,&py,&qx,&qy);
        rx = (2*qx)-px;
        ry = (2*qy)-py;
        cout<<rx<<" "<<ry<<endl;
        n--;
    }

    return 0;
}
