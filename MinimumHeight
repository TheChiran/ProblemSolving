height of triangle formular, height = 2*(area/b)

#include <iostream>
#include <cmath>
using namespace std;

int calculateHeight(int area,float base){
    return ceil(2*(area/base));
}

int main()
{
    int area;
    float base;
    long long int height;
    scanf("%f",&base);
    scanf("%d",&area);
    height = calculateHeight(area,base);
    cout<<height<<endl;
    return 0;
}
