height of triangle formular, height = 2*(area/b)

#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int area;
    float base;
    long long int height;
    scanf("%f",&base);
    scanf("%d",&area);
    height = ceil(2*(area/base));
    cout<<height<<endl;
    

    return 0;
}
